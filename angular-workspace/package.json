{
  "name": "angular-workspace",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "postinstall": "npm run proxies-api:install",
    "proxies-api:generate": "npx openapi-generator-cli generate -i ../proxies-api.openapi.json -g typescript-angular --type-mappings AnyType=any --additional-properties=serviceSuffix=Api,npmName=@c4-soft/proxies-api,npmVersion=0.0.1,stringEnums=true,enumPropertyNaming=camelCase,supportsES6=true,withInterfaces=true --remove-operation-id-prefix -o projects/c4-soft/proxies-api",
    "proxies-api:build": "npm run proxies-api:generate && npm run ng -- build @c4-soft/proxies-api --configuration production",
    "proxies-api:install": "cd projects/c4-soft/proxies-api && npm i && cd ../../.. && npm run proxies-api:build",
    "users-proxies:serve": "cd projects/users-proxies && npm run serve",
    "users-proxies:android": "cd projects/users-proxies && npm run android",
    "users-proxies:build": "cd projects/users-proxies && npm run build",
    "users-proxies:test": "cd projects/users-proxies && npm run test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/cdk": "^13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/material": "^13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "angular-oauth2-oidc": "^13.0.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.0",
    "@angular/cli": "~13.3.0",
    "@angular/compiler-cli": "~13.3.0",
    "@ionic/angular-toolkit": "^6.1.0",
    "@openapitools/openapi-generator-cli": "^2.4.26",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^13.0.0",
    "typescript": "~4.6.2"
  }
}
